{
    "version": 2,
    "builds": [
        {
            "src": "public/build/**",
            "use": "@vercel/static"
        },
        {
            "src": "public/images/**",
            "use": "@vercel/static"
        },
        {
            "src": "public/favicon.ico",
            "use": "@vercel/static"
        },
        {
            "src": "api/index.php",
            "use": "vercel-php@0.7.3"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/favicon\\.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
