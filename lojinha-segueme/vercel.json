{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.7.3"
        },
        {
            "src": "api/assets.php",
            "use": "vercel-php@0.7.3"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/api/assets.php"
        },
        {
            "src": "/images/(.*)",
            "dest": "/api/assets.php"
        },
        {
            "src": "/favicon.ico",
            "dest": "/api/assets.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
